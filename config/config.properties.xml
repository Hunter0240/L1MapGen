<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
<comment>MapGen config</comment>

<!-- Specify the Lineage client's map directory. -->
<entry key="LineageMapDirectory">Z:/L1Tools/MapGen/data/</entry>

<!-- Specify the map file output directory. -->
<entry key="OutputDirectory">Z:/L1Tools/MapGen/output/</entry>

<!-- Output V1 text map files. yes|no -->
<entry key="OutputV1Maps">yes</entry>

<!-- Output V2 map files (testing). yes|no -->
<entry key="OutputV2Maps">yes</entry>

<!-- Output MAP_INFO definition Java code for TextMapReader. yes|no -->
<entry key="OutputMapInfo">yes</entry>

<!-- Output CSV data for the map_ids table. yes|no -->
<entry key="OutputMapIds">yes</entry>

<!-- Path of the zone file to use when outputting map_ids.csv -->
<entry key="ZoneFilePath">Z:/L1Tools/MapGen/config/zone_en_US.txt</entry>

<!-- List all map tile values and output them. For testing. yes|no -->
<entry key="OutputTileValue">yes</entry>

</properties>
